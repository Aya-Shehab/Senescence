      <!-- new-products-section -->
      <section class="shop-section" style="background-image: url('/img/banner/testimonial-2.jpg');">
        <div class="auto-container">
          <div class="sec-title">
            <p>Our Shop</p>
            <h2>Our all New <br />Products</h2>
          </div>
          <div
            class="three-item-carousel owl-carousel owl-theme owl-dots-none nav-style-one centred"
          >
            <% if (products && products.length > 0) { %>
              <% products.slice(0, 3).forEach(product => { %>
                <div class="shop-block-one" style="border: 15px solid #add8d3;">
                  <div class="inner-box">
                    <figure class="image-box">
                      <img src="<%= product.imageUrl %>" alt="<%= product.name %>" />
                    </figure>
                    <div class="lower-content">
                      <span class="price-box">$<%= product.pricePackWhole %></span>
                      <h3><a href="/product/<%= product._id %>"><%= product.name %></a></h3>
                      <div class="cart-btn">
                        <a href="/product/<%= product._id %>" class="theme-btn-two">View details</a>
                      </div>
                    </div>
                  </div>
                </div>
              <% }); %>
            <% } else { %>
              <div class="no-products">
                <p>No products available at the moment.</p>
              </div>
            <% } %>
          </div>
        </div>
      </section>
